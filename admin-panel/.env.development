# 开发环境配置
NODE_ENV=development

# API 基础地址
VITE_API_BASE_URL=http://localhost:3000/api/v1

# 应用标题
VITE_APP_TITLE=图书馆管理系统

# 应用描述
VITE_APP_DESCRIPTION=现代化图书馆管理解决方案

# 应用版本
VITE_APP_VERSION=1.0.0

# 是否启用Mock数据
VITE_USE_MOCK=false

# 是否启用PWA
VITE_USE_PWA=false

# 上传文件大小限制（MB）
VITE_UPLOAD_SIZE_LIMIT=10

# 支持的图片格式
VITE_IMAGE_FORMATS=jpg,jpeg,png,gif,webp

# 支持的文档格式
VITE_DOCUMENT_FORMATS=pdf,doc,docx,txt

# CDN地址（开发环境为空）
VITE_CDN_URL=

# 是否启用调试模式
VITE_DEBUG=true

# 日志级别
VITE_LOG_LEVEL=debug